-- ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
-- // SE√á√ÉO 6: GERENCIAMENTO DE FAVORITOS (PERSIST√äNCIA)
-- ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

local KEY_FILE_NAME = "KhalexisHub_SavedKey.txt"
local FAV_FILE_NAME = "KhalexisHub_FavoritesV2.json"
local FavoritesList = {} 
local AllScriptsGlobal = {}
local MainFrameContainer, ToggleButton, HubOpen, HasShownWelcome

if isfile(FAV_FILE_NAME) then
    pcall(function()
        local content = readfile(FAV_FILE_NAME)
        local data = HttpService:JSONDecode(content)
        if typeof(data) == "table" then FavoritesList = data end
    end)
end

local function SaveFavorites()
    pcall(function() writefile(FAV_FILE_NAME, HttpService:JSONEncode(FavoritesList)) end)
end

local function IsFavorited(name)
    for _, v in ipairs(FavoritesList) do if v == name then return true end end
    return false
end

local function ToggleFavorite(name)
    if IsFavorited(name) then
        for i, v in ipairs(FavoritesList) do if v == name then table.remove(FavoritesList, i) break end end
        KhalexisToast("Favoritos", name .. " removido.", "Info", 2)
    else
        table.insert(FavoritesList, name)
        KhalexisToast("Favoritos", name .. " salvo!", "Success", 2)
    end
    SaveFavorites()
end

local function RegisterScript(name, func)
    local found = false
    for _, s in ipairs(AllScriptsGlobal) do if s.Name == name then found = true; break end end
    if not found then table.insert(AllScriptsGlobal, {Name = name, Func = func}) end
end

-- ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
-- // SE√á√ÉO 7: CONSTRU√á√ÉO DO HUB (UI PRINCIPAL)
-- ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

local function CreateHubInterface()
    local AnimContainer = Instance.new("Frame")
    AnimContainer.Name = "AnimationContainer"
    AnimContainer.Size = UDim2.new(0, 450, 0, 340)
    AnimContainer.Position = UDim2.new(0.5, 0, 0.5, 0)
    AnimContainer.AnchorPoint = Vector2.new(0.5, 0.5)
    AnimContainer.BackgroundTransparency = 1
    AnimContainer.Parent = ScreenGui

    local UIScale = Instance.new("UIScale")
    UIScale.Parent = AnimContainer
    UIScale.Scale = 0
    TweenService:Create(UIScale, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Scale = 1}):Play()

    local Main = Instance.new("Frame")
    Main.Name = "MainFrame"
    Main.Size = UDim2.fromScale(1,1)
    Main.BackgroundColor3 = Theme.Main.Background
    Main.Parent = AnimContainer
    ApplyCorner(Main, 12)
    ApplyStroke(Main, Theme.Main.Stroke, 2)

    -- Drag System (Arrastar Janela)
    local dragging, dragInput, dragStart, startPos
    Main.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true; dragStart = input.Position; startPos = AnimContainer.Position
        end
    end)
    Main.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement then dragInput = input end
    end)
    UserInputService.InputChanged:Connect(function(input)
        if dragging and input == dragInput then
            local delta = input.Position - dragStart
            AnimContainer.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end
    end)
    Main.InputEnded:Connect(function(input) if input.UserInputType == Enum.UserInputType.MouseButton1 then dragging = false end end)

    -- Barra Superior
    local TitleBar = Instance.new("Frame")
    TitleBar.Size = UDim2.new(1, -10, 0, 40)
    TitleBar.Position = UDim2.new(0, 5, 0, 5)
    TitleBar.BackgroundColor3 = Theme.Main.Sidebar
    TitleBar.Parent = Main
    ApplyCorner(TitleBar, 8)

    local TitleLbl = Instance.new("TextLabel")
    TitleLbl.Size = UDim2.fromScale(1,1)
    TitleLbl.BackgroundTransparency = 1
    TitleLbl.Text = "Khalexis Hub v6.0"
    TitleLbl.TextColor3 = Theme.Main.Accent
    TitleLbl.Font = HUB_FONT
    TitleLbl.TextSize = 18
    TitleLbl.Parent = TitleBar

    local CloseBtn = Instance.new("TextButton")
    CloseBtn.Size = UDim2.new(0, 30, 0, 30)
    CloseBtn.Position = UDim2.new(1, -35, 0.5, -15)
    CloseBtn.BackgroundColor3 = Theme.Status.Error
    CloseBtn.Text = "X"
    CloseBtn.TextColor3 = Color3.new(1,1,1)
    CloseBtn.Font = HUB_FONT
    CloseBtn.TextSize = 14
    CloseBtn.Parent = TitleBar
    ApplyCorner(CloseBtn, 8)
    
    CloseBtn.MouseButton1Click:Connect(function()
        local tween = TweenService:Create(UIScale, TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.In), {Scale = 0})
        tween:Play()
        tween.Completed:Wait()
        AnimContainer:Destroy()
        MainFrameContainer = nil
        HubOpen = false
        if ToggleButton then ToggleButton.Visible = true end
        KhalexisToast("Hub", "Interface minimizada", "Info", 2)
    end)

    -- Barra Lateral
    local Sidebar = Instance.new("Frame")
    Sidebar.Size = UDim2.new(0, 130, 1, -55)
    Sidebar.Position = UDim2.new(0, 5, 0, 50)
    Sidebar.BackgroundColor3 = Theme.Main.Background
    Sidebar.BackgroundTransparency = 1
    Sidebar.Parent = Main

    local SearchBox = Instance.new("TextBox")
    SearchBox.Size = UDim2.new(1, 0, 0, 25)
    SearchBox.BackgroundColor3 = Theme.Main.Sidebar
    SearchBox.TextColor3 = Theme.Text.Primary
    SearchBox.PlaceholderText = "Pesquisar..."
    SearchBox.PlaceholderColor3 = Theme.Text.Dim
    SearchBox.Font = TEXT_FONT
    SearchBox.TextSize = 12
    SearchBox.Parent = Sidebar
    ApplyCorner(SearchBox, 6)
    ApplyStroke(SearchBox, Theme.Main.Stroke, 1)

    local FavBtn = Instance.new("TextButton")
    FavBtn.Size = UDim2.new(1, 0, 0, 25)
    FavBtn.Position = UDim2.new(0, 0, 0, 30)
    FavBtn.BackgroundColor3 = Theme.Main.Sidebar
    FavBtn.TextColor3 = Theme.Text.Secondary
    FavBtn.Text = "‚≠ê Favoritos"
    FavBtn.Font = HUB_FONT
    FavBtn.TextSize = 12
    FavBtn.Parent = Sidebar
    ApplyCorner(FavBtn, 6)
    ApplyStroke(FavBtn, Theme.Main.Stroke, 1)

    local CatScroll = Instance.new("ScrollingFrame")
    CatScroll.Size = UDim2.new(1, 0, 1, -60)
    CatScroll.Position = UDim2.new(0, 0, 0, 60)
    CatScroll.BackgroundTransparency = 1
    CatScroll.ScrollBarThickness = 2
    CatScroll.ScrollBarImageColor3 = Theme.Main.Stroke
    CatScroll.Parent = Sidebar
    
    local CatLayout = Instance.new("UIListLayout")
    CatLayout.Padding = UDim.new(0, 5)
    CatLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
    CatLayout.Parent = CatScroll

    -- √Årea de Scripts
    local ScriptArea = Instance.new("ScrollingFrame")
    ScriptArea.Size = UDim2.new(1, -145, 1, -55)
    ScriptArea.Position = UDim2.new(0, 140, 0, 50)
    ScriptArea.BackgroundColor3 = Theme.Main.Sidebar
    ScriptArea.ScrollBarThickness = 2
    ScriptArea.ScrollBarImageColor3 = Theme.Main.Stroke
    ScriptArea.Parent = Main
    ApplyCorner(ScriptArea, 8)
    
    local ScriptLayout = Instance.new("UIListLayout")
    ScriptLayout.Padding = UDim.new(0, 5)
    ScriptLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
    ScriptLayout.Parent = ScriptArea
    local ScriptPad = Instance.new("UIPadding")
    ScriptPad.PaddingTop = UDim.new(0, 10)
    ScriptPad.Parent = ScriptArea

    SearchBox:GetPropertyChangedSignal("Text"):Connect(function()
        local txt = SearchBox.Text:lower()
        for _, btn in pairs(CatScroll:GetChildren()) do
            if btn:IsA("TextButton") then
                btn.Visible = string.find(btn.Text:lower(), txt)
            end
        end
        CatScroll.CanvasSize = UDim2.new(0, 0, 0, CatLayout.AbsoluteContentSize.Y)
    end)

    local function ClearContent() for _,v in pairs(ScriptArea:GetChildren()) do if v:IsA("GuiObject") then v:Destroy() end end end

    -- [DASHBOARD SYSTEM]
    local function CreateDashboard()
        ClearContent()
        local DashFrame = Instance.new("Frame")
        DashFrame.Size = UDim2.new(1,0,1,0); DashFrame.BackgroundTransparency=1; DashFrame.Parent=ScriptArea
        
        -- Header do Perfil
        local Header = Instance.new("Frame")
        Header.Size=UDim2.new(1,0,0,80); Header.BackgroundColor3=Theme.Main.Background; Header.Parent=DashFrame
        ApplyCorner(Header,8)
        ApplyStroke(Header, Theme.Main.Stroke, 1, 0.5)

        local Avatar = Instance.new("ImageLabel")
        Avatar.Size=UDim2.new(0,60,0,60); Avatar.Position=UDim2.new(0,10,0.5,-30); Avatar.BackgroundColor3=Theme.Main.Sidebar
        Avatar.Image = Players:GetUserThumbnailAsync(userId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size100x100)
        Avatar.Parent=Header; ApplyCorner(Avatar,30)

        local Welc = Instance.new("TextLabel")
        Welc.Size=UDim2.new(1,-80,0,20); Welc.Position=UDim2.new(0,80,0,20); Welc.BackgroundTransparency=1
        Welc.TextColor3=Theme.Text.Primary; Welc.Font=HUB_FONT; Welc.TextSize=18; Welc.TextXAlignment=Enum.TextXAlignment.Left
        Welc.Parent=Header

        local Info = Instance.new("TextLabel")
        Info.Size=UDim2.new(1,-80,0,20); Info.Position=UDim2.new(0,80,0,45); Info.BackgroundTransparency=1
        Info.TextColor3=Theme.Text.Secondary; Info.Font=TEXT_FONT; Info.TextSize=12; Info.TextXAlignment=Enum.TextXAlignment.Left
        Info.Text="Executor: "..(identifyexecutor and identifyexecutor() or "Unknown"); Info.Parent=Header

        -- Status Grid
        local Stats = Instance.new("Frame")
        Stats.Size=UDim2.new(1,0,0,100); Stats.Position=UDim2.new(0,0,0,90); Stats.BackgroundColor3=Theme.Main.Background; Stats.Parent=DashFrame
        ApplyCorner(Stats,8); ApplyStroke(Stats, Theme.Main.Stroke, 1, 0.5)

        local function mkStat(t, v, p)
            local f = Instance.new("Frame"); f.Size=UDim2.new(0.48,0,0,40); f.Position=p; f.BackgroundColor3=Theme.Main.Sidebar; f.Parent=Stats; ApplyCorner(f,6)
            local l1 = Instance.new("TextLabel"); l1.Size=UDim2.new(1,-10,1,0); l1.Position=UDim2.new(0,10,0,0); l1.BackgroundTransparency=1; l1.TextColor3=Theme.Text.Dim; l1.TextXAlignment=Enum.TextXAlignment.Left; l1.Font=TEXT_FONT; l1.TextSize=12; l1.Text=t; l1.Parent=f
            local l2 = Instance.new("TextLabel"); l2.Size=UDim2.new(1,-10,1,0); l2.BackgroundTransparency=1; l2.TextColor3=Theme.Main.Accent; l2.TextXAlignment=Enum.TextXAlignment.Right; l2.Font=HUB_FONT; l2.TextSize=12; l2.Text=v; l2.Parent=f
            return l2
        end
        local pLabel = mkStat("Ping", "0ms", UDim2.new(0,0,0,10))
        local fLabel = mkStat("FPS", "0", UDim2.new(0.52,0,0,10))
        local tLabel = mkStat("Time", "00:00", UDim2.new(0,0,0,55))
        local gLabel = mkStat("Game", "...", UDim2.new(0.52,0,0,55))
        
        pcall(function() gLabel.Text = MarketplaceService:GetProductInfo(game.PlaceId).Name end)

        spawn(function()
            while DashFrame.Parent do
                local ping = "0"
                pcall(function() ping = math.floor(StatsService.Network.ServerStatsItem["Data Ping"]:GetValueString():split(" ")[1]) end)
                local fps = math.floor(workspace:GetRealPhysicsFPS())
                local date = os.date("*t")
                if date.hour < 12 then Welc.Text = "Bom dia, "..Player.Name elseif date.hour < 18 then Welc.Text = "Boa tarde, "..Player.Name else Welc.Text = "Boa noite, "..Player.Name end
                pLabel.Text = ping.." ms"; fLabel.Text = fps; tLabel.Text = string.format("%02d:%02d:%02d", date.hour, date.min, date.sec)
                wait(1)
            end
        end)
    end

    local function createScriptButton(name, func)
        RegisterScript(name, func)
        local btn = Instance.new("TextButton")
        btn.Size = UDim2.new(1, -20, 0, 30)
        btn.BackgroundColor3 = Theme.Main.Background
        btn.Text = "      "..name:upper()
        btn.TextColor3 = Theme.Text.Primary
        btn.Font = HUB_FONT
        btn.TextSize = 12
        btn.TextXAlignment = Enum.TextXAlignment.Left
        btn.Parent = ScriptArea
        ApplyCorner(btn, 6)
        ApplyStroke(btn, Theme.Main.Stroke, 1)

        local Icon = Instance.new("ImageLabel")
        Icon.Size = UDim2.new(0, 20, 0, 20)
        Icon.Position = UDim2.new(0, 5, 0.5, -10)
        Icon.BackgroundTransparency = 1
        Icon.Image = Assets.Icons.Finger
        Icon.ImageColor3 = Color3.fromRGB(150,150,150)
        Icon.Parent = btn

        local Star = Instance.new("TextButton")
        Star.Size = UDim2.new(0, 30, 1, 0)
        Star.Position = UDim2.new(1, -30, 0, 0)
        Star.BackgroundTransparency = 1
        Star.Text = "‚òÖ"
        Star.TextSize = 18
        Star.TextColor3 = IsFavorited(name) and Colors.StarOn or Colors.StarOff
        Star.Parent = btn

        Star.MouseButton1Click:Connect(function()
            ToggleFavorite(name)
            Star.TextColor3 = IsFavorited(name) and Colors.StarOn or Colors.StarOff
        end)

        btn.MouseButton1Click:Connect(function()
            KhalexisToast("Executando", "Injetando: " .. name, "Info", 2)
            local s, e = pcall(func)
            if s then
                KhalexisToast("Sucesso", name.." carregado!", "Success", 3)
            else
                KhalexisToast("Erro", e, "Error", 5)
            end
        end)
    end

    local function createCategoryButton(name, contents, order)
        local Btn = Instance.new("TextButton")
        Btn.Size = UDim2.new(1, -10, 0, 25)
        Btn.BackgroundColor3 = Theme.Main.Sidebar
        Btn.Text = name
        Btn.TextColor3 = Theme.Text.Secondary
        Btn.Font = HUB_FONT
        Btn.TextSize = 11
        Btn.TextWrapped = true
        Btn.LayoutOrder = order or 10
        Btn.Parent = CatScroll
        ApplyCorner(Btn, 6)

        if name == "Dashboard" then
            Btn.Text = "‚òÖ Dashboard"
            Btn.TextColor3 = Theme.Main.Accent
            Btn.LayoutOrder = 1
        elseif DetectedGameCategory and name == DetectedGameCategory then
            Btn.Text = "‚òÖ "..name
            Btn.TextColor3 = Theme.Main.Accent
            Btn.LayoutOrder = 2
        end

        Btn.MouseButton1Click:Connect(function()
            ClearContent()
            for _,b in pairs(CatScroll:GetChildren()) do if b:IsA("TextButton") then b.TextColor3 = Theme.Text.Secondary end end
            Btn.TextColor3 = Theme.Main.Accent
            
            if name == "Dashboard" then
                CreateDashboard()
            elseif name == "üì£ Atualiza√ß√µes üì£" then
                -- Updates text logic
                 local lbl=Instance.new("TextLabel"); lbl.Size=UDim2.new(1,0,0,30); lbl.BackgroundTransparency=1; lbl.TextColor3=Theme.Text.Primary; lbl.Font=HUB_FONT; lbl.Text="Bem Vindo √† v6.0!"; lbl.Parent=ScriptArea
            elseif contents then
                for _, s in ipairs(contents) do createScriptButton(s.Name, s.Func) end
            end
            ScriptArea.CanvasSize = UDim2.new(0, 0, 0, ScriptLayout.AbsoluteContentSize.Y + 20)
        end)
        return Btn
    end

    -- L√≥gica Favoritos
    FavBtn.MouseButton1Click:Connect(function()
        ClearContent()
        for _,b in pairs(CatScroll:GetChildren()) do if b:IsA("TextButton") then b.TextColor3 = Theme.Text.Secondary end end
        FavBtn.TextColor3 = Theme.Main.Accent
        local hasFav = false
        for _, favName in ipairs(FavoritesList) do
            local scriptData
            for _, s in ipairs(AllScriptsGlobal) do if s.Name == favName then scriptData = s break end end
            if scriptData then createScriptButton(scriptData.Name, scriptData.Func); hasFav = true end
        end
        if not hasFav then 
             local lbl=Instance.new("TextLabel"); lbl.Size=UDim2.new(1,0,0,30); lbl.BackgroundTransparency=1; lbl.TextColor3=Theme.Text.Dim; lbl.Font=TEXT_FONT; lbl.Text="Voc√™ n√£o tem favoritos ainda."; lbl.Parent=ScriptArea
        end
        ScriptArea.CanvasSize = UDim2.new(0, 0, 0, ScriptLayout.AbsoluteContentSize.Y + 20)
    end)

