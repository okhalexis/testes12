local HUB_FONT = Enum.Font.FredokaOne 
local TEXT_FONT = Enum.Font.GothamMedium
local TITLE_SIZE = 18
local BODY_SIZE = 14

local Theme = {
    Main = {
        Background = Color3.fromRGB(15, 15, 15),
        Sidebar = Color3.fromRGB(20, 20, 20),
        Content = Color3.fromRGB(25, 25, 25),
        Stroke = Color3.fromRGB(60, 60, 60),
        Accent = Color3.fromRGB(255, 140, 0), -- Laranja Assinatura
    },
    Text = {
        Primary = Color3.fromRGB(255, 255, 255),
        Secondary = Color3.fromRGB(180, 180, 180),
        Dim = Color3.fromRGB(120, 120, 120),
    },
    Status = {
        Success = Color3.fromRGB(40, 200, 64),
        Error = Color3.fromRGB(245, 70, 90),
        Warning = Color3.fromRGB(254, 188, 46),
        Info = Color3.fromRGB(59, 130, 246)
    },
    Interactive = {
        Hover = Color3.fromRGB(40, 40, 40),
        Click = Color3.fromRGB(50, 50, 50)
    }
}

local Assets = {
    Logo = "rbxassetid://101462084575687",
    Icons = {
        Home = "rbxassetid://11295288868",
        Script = "rbxassetid://12333784627",
        Key = "rbxassetid://114355063515473",
        Warning = "rbxassetid://130226573962640",
        Success = "rbxassetid://119783053916823",
        Error = "rbxassetid://113312905787220",
        Info = "rbxassetid://94529541997278"
    }
}

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "KhalexisHub"
ScreenGui.ResetOnSpawn = false
ScreenGui.Parent = CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

-- ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
-- // SE√á√ÉO 3: FUN√á√ïES DE UTILIDADE E UI (FRAMEWORK)
-- ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

local function ApplyCorner(instance, radius)
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, radius or 8)
    corner.Parent = instance
    return corner
end

local function ApplyStroke(instance, color, thickness, transparency)
    local stroke = Instance.new("UIStroke")
    stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    stroke.Color = color or Theme.Main.Stroke
    stroke.Thickness = thickness or 1
    stroke.Transparency = transparency or 0
    stroke.Parent = instance
    return stroke
end

local function ApplyGradient(instance, color1, color2)
    local gradient = Instance.new("UIGradient")
    gradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, color1 or Color3.fromRGB(255, 255, 255)),
        ColorSequenceKeypoint.new(1, color2 or Color3.fromRGB(150, 150, 150))
    }
    gradient.Rotation = 45
    gradient.Parent = instance
    return gradient
end

local function CreateLabel(parent, text, size, pos, color, font)
    local label = Instance.new("TextLabel")
    label.Size = size or UDim2.new(1, 0, 1, 0)
    label.Position = pos or UDim2.new(0, 0, 0, 0)
    label.BackgroundTransparency = 1
    label.Text = text or ""
    label.TextColor3 = color or Theme.Text.Primary
    label.Font = font or HUB_FONT
    label.TextSize = 14
    label.Parent = parent
    return label
end

-- ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
-- // SE√á√ÉO 4: SISTEMA DE NOTIFICA√á√ÉO (TOAST ENGINE)
-- ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

local NotificationHolder = Instance.new("Frame")
NotificationHolder.Name = "NotificationHolder"
NotificationHolder.Size = UDim2.new(0, 320, 1, 0)
NotificationHolder.Position = UDim2.new(0.5, -160, 0.85, 0) -- Posicionado abaixo do centro
NotificationHolder.BackgroundTransparency = 1
NotificationHolder.Parent = ScreenGui
NotificationHolder.ZIndex = 5000

local NotifList = Instance.new("UIListLayout")
NotifList.Parent = NotificationHolder
NotifList.SortOrder = Enum.SortOrder.LayoutOrder
NotifList.Padding = UDim.new(0, 10)
NotifList.HorizontalAlignment = Enum.HorizontalAlignment.Center
NotifLayout = Enum.VerticalAlignment.Bottom
NotifList.VerticalAlignment = Enum.VerticalAlignment.Bottom

function KhalexisToast(title, text, type, duration)
    -- Defini√ß√£o de Estilo baseado no Tipo
    local accentColor = Theme.Main.Accent
    local iconId = Assets.Icons.Info
    
    if type == "Success" then
        accentColor = Theme.Status.Success
        iconId = Assets.Icons.Success
    elseif type == "Error" then
        accentColor = Theme.Status.Error
        iconId = Assets.Icons.Error
    elseif type == "Warning" then
        accentColor = Theme.Status.Warning
        iconId = Assets.Icons.Warning
    end

    -- Container Principal
    local Toast = Instance.new("Frame")
    Toast.Name = "ToastNotification"
    Toast.Size = UDim2.new(1, 0, 0, 60)
    Toast.BackgroundColor3 = Theme.Main.Background
    Toast.BackgroundTransparency = 0.1
    Toast.BorderSizePixel = 0
    Toast.Parent = NotificationHolder
    ApplyCorner(Toast, 12)
    local Stroke = ApplyStroke(Toast, accentColor, 1.5)
    Stroke.Transparency = 0.5

    -- √çcone com Fundo Circular
    local IconContainer = Instance.new("Frame")
    IconContainer.Size = UDim2.new(0, 40, 0, 40)
    IconContainer.Position = UDim2.new(0, 10, 0.5, -20)
    IconContainer.BackgroundColor3 = accentColor
    IconContainer.BackgroundTransparency = 0.85
    IconContainer.Parent = Toast
    ApplyCorner(IconContainer, 20)

    local IconImg = Instance.new("ImageLabel")
    IconImg.Size = UDim2.new(0, 24, 0, 24)
    IconImg.Position = UDim2.new(0.5, -12, 0.5, -12)
    IconImg.BackgroundTransparency = 1
    IconImg.Image = iconId
    IconImg.ImageColor3 = accentColor
    IconImg.Parent = IconContainer

    -- T√≠tulo
    local TitleLabel = CreateLabel(Toast, title, UDim2.new(1, -60, 0, 20), UDim2.new(0, 60, 0, 8), Theme.Text.Primary, HUB_FONT)
    TitleLabel.TextXAlignment = Enum.TextXAlignment.Left

    -- Descri√ß√£o
    local DescLabel = CreateLabel(Toast, text, UDim2.new(1, -60, 0, 20), UDim2.new(0, 60, 0, 28), Theme.Text.Secondary, TEXT_FONT)
    DescLabel.TextXAlignment = Enum.TextXAlignment.Left
    DescLabel.TextSize = 12
    DescLabel.TextWrapped = true

    -- Barra de Progresso (Tempo)
    local TimeBar = Instance.new("Frame")
    TimeBar.Size = UDim2.new(1, 0, 0, 2)
    TimeBar.Position = UDim2.new(0, 0, 1, -2)
    TimeBar.BackgroundColor3 = accentColor
    TimeBar.BorderSizePixel = 0
    TimeBar.Parent = Toast
    ApplyCorner(TimeBar, 2)

    -- Anima√ß√£o de Entrada
    Toast.BackgroundTransparency = 1
    TitleLabel.TextTransparency = 1
    DescLabel.TextTransparency = 1
    IconImg.ImageTransparency = 1
    TimeBar.BackgroundTransparency = 1
    
    local animInfo = TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.Out)
    TweenService:Create(Toast, animInfo, {BackgroundTransparency = 0.1}):Play()
    TweenService:Create(TitleLabel, animInfo, {TextTransparency = 0}):Play()
    TweenService:Create(DescLabel, animInfo, {TextTransparency = 0}):Play()
    TweenService:Create(IconImg, animInfo, {ImageTransparency = 0}):Play()
    TweenService:Create(TimeBar, animInfo, {BackgroundTransparency = 0}):Play()
    
    -- Anima√ß√£o da Barra de Tempo
    TweenService:Create(TimeBar, TweenInfo.new(duration, Enum.EasingStyle.Linear), {Size = UDim2.new(0, 0, 0, 2)}):Play()

    -- Destrui√ß√£o Autom√°tica
    task.delay(duration, function()
        if Toast and Toast.Parent then
            local outInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
            TweenService:Create(Toast, outInfo, {BackgroundTransparency = 1, Size = UDim2.new(1, 0, 0, 0)}):Play()
            TweenService:Create(TitleLabel, outInfo, {TextTransparency = 1}):Play()
            TweenService:Create(DescLabel, outInfo, {TextTransparency = 1}):Play()
            TweenService:Create(IconImg, outInfo, {ImageTransparency = 1}):Play()
            wait(0.3)
            Toast:Destroy()
        end
    end)
end

-- Override global para compatibilidade
_G.showAutoMessage = function(msg)
    KhalexisToast("Aviso do Sistema", msg, "Info", 4)
end

