local function showLoadingAndOpenHub()
    LoadingScreen, StopLoadingAnim = showLoadingScreen()
    wait(2)
    if StopLoadingAnim then StopLoadingAnim() end
    if LoadingScreen and LoadingScreen.Parent then LoadingScreen:Destroy() end

    if not HasShownWelcome then
        KhalexisToast("Bem Vindo", "Bem vindo ao Khalexis Hub v6.0!", "Success", 5)
        HasShownWelcome = true
    end

    MainFrameContainer=createHub()
    HubOpen=true
    if ToggleButton then ToggleButton.Visible = false end
end

-- ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
-- // SE√á√ÉO 8: KEY SYSTEM UI (LINKUNLOCKER INTEGRADO)
-- ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

local Config = {
    api = "e696e91d-0ba0-4693-a6a5-91145ead6ed1", 
    service = "Khalexis Hub - Key",
    provider = "Khalexis Hub - Key"
}

local function CreateKeyUI()
    local KeyMain = Instance.new("Frame"); KeyMain.Size=UDim2.new(0,350,0,250); KeyMain.Position=UDim2.new(0.5,0,0.5,0); KeyMain.AnchorPoint=Vector2.new(0.5,0.5); KeyMain.BackgroundColor3=Theme.Main.Background; KeyMain.Parent=ScreenGui; ApplyCorner(KeyMain,12); ApplyStroke(KeyMain, Theme.Main.Accent, 2)
    local Title = Instance.new("TextLabel"); Title.Text="Khalexis Key System"; Title.Size=UDim2.new(1,0,0,50); Title.BackgroundTransparency=1; Title.TextColor3=Theme.Main.Accent; Title.Font=HUB_FONT; Title.TextSize=20; Title.Parent=KeyMain
    local KeyInput = Instance.new("TextBox"); KeyInput.Size=UDim2.new(0,280,0,40); KeyInput.Position=UDim2.new(0.5,-140,0.4,0); KeyInput.BackgroundColor3=Theme.Main.Content; KeyInput.TextColor3=Theme.Text.Primary; KeyInput.PlaceholderText="Cole sua key..."; KeyInput.Text=""; KeyInput.Parent=KeyMain; ApplyCorner(KeyInput,8)
    local BtnGet = Instance.new("TextButton"); BtnGet.Size=UDim2.new(0,120,0,35); BtnGet.Position=UDim2.new(0.5,10,0.65,0); BtnGet.BackgroundColor3=Theme.Main.Sidebar; BtnGet.Text="Pegar Key"; BtnGet.TextColor3=Theme.Text.Primary; BtnGet.Font=HUB_FONT; BtnGet.TextSize=14; BtnGet.Parent=KeyMain; ApplyCorner(BtnGet,8)
    local BtnCheck = Instance.new("TextButton"); BtnCheck.Size=UDim2.new(0,120,0,35); BtnCheck.Position=UDim2.new(0.5,-130,0.65,0); BtnCheck.BackgroundColor3=Theme.Main.Accent; BtnCheck.Text="Verificar"; BtnCheck.TextColor3=Theme.Main.Background; BtnCheck.Font=HUB_FONT; BtnCheck.TextSize=14; BtnCheck.Parent=KeyMain; ApplyCorner(BtnCheck,8)

    BtnGet.MouseButton1Click:Connect(function()
        setclipboard("https://linkunlocker.com/key-1-dS7uj?share=true")
        KhalexisToast("Link", "Link copiado para a √°rea de transfer√™ncia!", "Success", 3)
    end)

    BtnCheck.MouseButton1Click:Connect(function()
        local key = KeyInput.Text:gsub("%s+", "")
        KhalexisToast("Verificando", "Validando...", "Info", 2)
        local valid = false
        pcall(function()
            local Junkie = loadstring(game:HttpGet("https://junkie-development.de/sdk/JunkieKeySystem.lua"))()
            valid = Junkie.verifyKey(Config.api, key, Config.service)
        end)
        if valid then
            writefile(KEY_FILE_NAME, key)
            KhalexisToast("Sucesso", "Acesso permitido!", "Success", 3)
            KeyMain:Destroy()
            MainFrameContainer = createHub()
            HubOpen = true
            ToggleButton.Visible = false
        else
            KhalexisToast("Erro", "Key inv√°lida.", "Error", 3)
        end
    end)
end

-- ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
-- // SE√á√ÉO 9: TOGGLE BUTTON (REDONDO & ARRAST√ÅVEL)
-- ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

ToggleButton = Instance.new("ImageButton")
ToggleButton.Name = "ToggleButton"
ToggleButton.Size = UDim2.new(0, 50, 0, 50)
ToggleButton.Position = UDim2.new(0, 20, 0, 100)
ToggleButton.BackgroundColor3 = Theme.Main.Sidebar
ToggleButton.Image = Assets.Logo
ToggleButton.Parent = ScreenGui
ToggleButton.Visible = false
local tc = Instance.new("UICorner"); tc.CornerRadius = UDim.new(1, 0); tc.Parent = ToggleButton
local ts = Instance.new("UIStroke"); ts.Color = Theme.Main.Accent; ts.Thickness = 2; ts.Parent = ToggleButton

local draggingT, dragStartT, startPosT, hasMovedT
ToggleButton.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        draggingT = true; hasMovedT = false; dragStartT = input.Position; startPosT = ToggleButton.Position
    end
end)
UserInputService.InputChanged:Connect(function(input)
    if draggingT and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
        local delta = input.Position - dragStartT
        if delta.Magnitude > 3 then
            hasMovedT = true
            ToggleButton.Position = UDim2.new(startPosT.X.Scale, startPosT.X.Offset + delta.X, startPosT.Y.Scale, startPosT.Y.Offset + delta.Y)
        end
    end
end)
ToggleButton.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        draggingT = false
        if not hasMovedT then
            if HubOpen then 
                if MainFrameContainer then MainFrameContainer:Destroy() end
                MainFrameContainer = nil
                HubOpen = false
                ToggleButton.Visible = true
                KhalexisToast("Hub", "Fechado", "Info", 1)
            else 
                MainFrameContainer = createHub()
                HubOpen = true
                ToggleButton.Visible = false
                KhalexisToast("Hub", "Aberto", "Info", 1)
            end
        end
    end
end)

local function Start()
    local keyValid = false
    if isfile(KEY_FILE_NAME) then
        local k = readfile(KEY_FILE_NAME):gsub("%s+", "")
        if k ~= "" then
             pcall(function()
                local Junkie = loadstring(game:HttpGet("https://junkie-development.de/sdk/JunkieKeySystem.lua"))()
                keyValid = Junkie.verifyKey(Config.api, k, Config.service)
            end)
        end
    end
    if keyValid then
        KhalexisToast("Bem Vindo", "Key carregada.", "Success", 3)
        main()
    else
        CreateKeyUI()
    end
end

Start()
